.type V

.decl Rule(n: number)
.input Rule

.decl actedin(v0: V, v1: V)
.input actedin

.decl ismarriedto(v0: V, v1: V)
.input ismarriedto

.decl created(v0: V, v1: V)
.input created

.decl produced(v0: V, v1: V)
.input produced

