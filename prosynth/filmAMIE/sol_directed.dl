.decl directed(v0: V, v1: V)
.output directed

directed(v0, v1) :- created(v0, v1), Rule(268).
directed(v0, v1) :- actedin(v0, v1), Rule(264).
directed(v0, v1) :- produced(v0, v1), Rule(264).
directed(v0, v1) :- actedin(v0, v1), produced(v0, v1).
directed(v0, v1) :- created(v0, v1), produced(v0, v1).
directed(v0, v1) :- ismarriedto(v0, v2), actedin(v2, v1).
